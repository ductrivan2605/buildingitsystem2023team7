<div class="navForBook">
    <% categories.forEach(category => { %>
    <div class="nav-items">
        <div class="nav-item">
            <a href="/category/<%= category.slug %>">
                <img class="itemImage"src="/images/<%= category.image %>" alt="Novel_icon">
                <div class="categoryName">
                    <p><%= category.category %></p>
                </div>
            </a>
        </div>
    </div>
    <% }) %>
    <div class="nav-items">
        <a href="/category/all" class="nav-item">
            <ion-icon class="viewMore" name="add-outline"></ion-icon>
            <p>View more</p>
        </a>
    </div>
    
    
</div>